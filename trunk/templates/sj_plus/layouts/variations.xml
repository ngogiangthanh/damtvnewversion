<?xml version="1.0" encoding="utf-8"?>
<layout type="mr">	
	<head>
	</head>
	<groups>
		<group class="span3" data-tablet="span6 first" data-stablet="">left</group>
		<group class="span3" data-tablet="span6" data-stablet="" >right</group>
		<group 
			   class="span6" 
			   data-tablet="span12"
			   
			   noright-class="span9"
			   noright-data-normal=" " 
			   noright-data-stablet="span9"
			   
			   noleft-class="span9 first"
			   noleft-data-normal="" 
			   noleft-data-stablet=""
			   
			   noleftright-class="span12"
			   noleftright-data-normal="" 
			   noleftright-data-tablet="" 
			   noleftright-data-stablet="">main</group>
	</groups>
	<blocks>			
		<header html5tag="header" order="1" id="yt_header" autosize="0">
			<positions>
				<position class="span3" data-tablet="span4"  type="feature">@logo</position>
				<position class="span3" data-tablet="span4"  data-mobile="span4"   style="none" type="modules">social</position>	
				<position class="span3"	data-tablet="hidden" data-mobile="hidden"  style="none" type="modules">search</position>	
				<position class="span3" data-tablet="span4"  data-mobile="hidden"  style="none" type="modules">contact</position>	
			</positions>
		</header>
		
		<menu html5tag="section" order="2" id="yt_menuwrap">
			<positions>
				<position class="span12"  type="feature">@menu</position>
			</positions>
		</menu>
		

		<spotlight1 html5tag="section" order="5" id="yt_spotlight1">
			<positions>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">top1</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">top2</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">top3</position>
				<position class="span3" data-normal="" data-tablet="" data-stablet="" style="ytmod" type="modules">top4</position>
			</positions>
		</spotlight1>	
		
		<content html5tag="section" order="6" id="content">
			<positions>
				
				<position class="span6 first" group="main" style="ytmod" type="modules">maintop-1</position>
				<position class="span6" group="main" style="ytmod" type="modules">maintop-2</position>

				<position class="span12 no-minheight" group="main" type="message"></position>
				<position class="span12" style="ytmod" group="main" type="modules">breadcrumb</position>
				<position 
					class="span12 first" 
					data-normal=" " 
					data-tablet="span12" 
					data-stablet="  "
					
					noright-class="span12" 
					noright-data-normal=" " 
					noright-data-tablet=" " 
					noright-data-stablet=" " 
					
					group="main" type="component">component</position>
				<position class="span6 first" group="main" style="ytmod" type="modules">mainbottom-1</position>
				<position class="span6" group="main" style="ytmod" type="modules">mainbottom-2</position>
	
				<position class="span12" data-normal="span12" data-tablet="span12" data-stablet="span12" group="left" style="ytmod" type="modules">left</position>
				<position class="span12" data-normal="span12" data-tablet="span12" data-stablet="span12" group="right" style="ytmod" type="modules">right</position>
			</positions>
		</content>
		
		<spotlight3 html5tag="section" order="8" id="yt_spotlight3">
			<positions>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom5</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom6</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom7</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom8</position>
			</positions>
		</spotlight3>
		
		<spotlight2 html5tag="section" order="9" id="yt_spotlight2">
			<positions>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom1</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom2</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom3</position>
				<position class="span3" data-normal="" data-stablet="" style="ytmod" type="modules">bottom4</position>
			</positions>
		</spotlight2>
		
		<footer html5tag="footer" order="10" id="yt_footer">
			<positions>
				<position class="span8" type="feature">@copyright</position>
				<position class="span4" type="modules">footer</position>			
			</positions>
		</footer>										
	</blocks>
</layout>






















